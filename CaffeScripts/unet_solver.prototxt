net: "unet_structure.prototxt"

# solver type and mode
type: "Adam"
solver_mode: GPU

# since Adam dynamically changes the learning rate, we set the base learning rate to a fixed value
lr_policy: "fixed"
base_lr: 0.01
momentum: 0.9
momentum2: 0.999

# Output intermediate results to console every N iterations
display: 100

# NOT NEEDED
# max_iter: 10000
# test_iter: 64
# test_interval: 640
# snapshot: 640
# snapshot_prefix: "/path/to/output/folder/somenameprefix"
